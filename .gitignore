# Dart & Flutter specific ignores

# Files and directories created by pub
.dart_tool/
.packages
build/
.pub-cache/
.pub/
pubspec.lock

# Directory created by dartdoc
**/doc/api/

# Flutter plugin-specific files
.flutter-plugins
.flutter-plugins-dependencies

# Android specific ignores
/android/app/debug
/android/app/profile
/android/app/release

# iOS specific ignores
**/ios/Flutter/.last_build_id

# dotenv environment variables files
.env*

# Ignore generated Javascript files
*.dart.js
*.info.json      # Produced by the --dump-info flag.
*.js             # When generated by dart2js. Don't specify *.js if your project includes source files written in JavaScript.
*.js_
*.js.deps
*.js.map

# Other common ignores
*.class
*.log
*.pyc
*.swp
.DS_Store
.atom/
.buildlog/
.history
.svn/
migrate_working_dir/

# IDEs and Editors
*.iml
*.ipr
*.iws
.idea/
.vscode/

# App specific
app.*.map.json
app.*.symbols